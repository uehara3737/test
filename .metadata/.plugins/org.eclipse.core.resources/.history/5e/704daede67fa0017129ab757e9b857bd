
public class HelloWorld {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

	
		
		int a=88;
		if(a<=100){
			if(a<20){
				System.out.println("未成年");
			}else if(a==77){
				System.out.println("喜寿");
			}else if(a==88){
				System.out.println("米寿");
			}else if(a>90){
				System.out.println("高齢者");
			}
			else{
				System.out.println("成人");
			}
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

